[pixel shader]
uniform float4 color;

float4 main() : SV_target
{
    float4 ret = color;
    ret.zwx.yzx.xz += float2(0.4, 0.2);
    ret.xzyw.wzx.xz.y.x -= 0.6;
    ret.yxwz.yxwz *= -1;
    return ret;
}

