[pixel shader]
struct stu
{
    float aa;
    float2 bb;
    float cc;
};

float4 main() : sv_target
{
    struct stu foo;
    foo.cc = 5;
    foo.bb = float3(2, 3, 4); // It shall not overflow to cc;
    foo.aa = 1;

    return float4(foo);
}

