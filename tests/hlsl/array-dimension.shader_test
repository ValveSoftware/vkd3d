    /* float array dimensions are not valid. */
    float b[4] = a;
#else
    float b[4.1] = a;
#endif
    float c[dim] = b;
    float d[true] = {c[0]};
    float e[65536];
    return float4(d[0], c[0], c[1], c[3]);
}

