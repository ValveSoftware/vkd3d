[pixel shader]
uniform float4 a;
uniform float2 b;

float4 main() : SV_TARGET
{
    float u = a.x, v = a.y, w = a.z, x = a.w, y = b.x, z = b.y;
    return float4(x * y - z / w + --u / -v,
            z * x / y + w / -v,
            u + v - w,
            x / y / w);
}

