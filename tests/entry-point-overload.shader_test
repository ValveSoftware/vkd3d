% The last function to be declared is the one that's used.

[vertex shader]
void main(out float tex : texcoord, inout float4 pos : sv_position)
{
    tex = 0.2;
}

[pixel shader]

float4 main() : sv_target
{
    return 0.1;
}

float4 main(float tex : texcoord) : sv_target
{
    return tex;
}

