[pixel shader]
struct apple
{
    float3 aa;
    float4 bb;
};

struct banana
{
    struct apple aa;
    int2 bb;
    int4 cc[8];
};

float4 main() : SV_TARGET
{
    struct banana p = (struct banana)42;
    return p.aa.bb + p.cc[5];
}

